pipeline {
    agent { docker {image 'maven:3.6.3'}}
    stages {
        stage ('build') {
            steps {
                sh 'mvn --version'
                echo "build"
            }
        }
        stage ('Test') {
            steps {
                echo "Test"
            }
        }
        stage ('integration test') {
            steps {
                echo "integration test"
            }
        }

}
    
}
