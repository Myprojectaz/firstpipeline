pipeline {
    // agent { docker { image 'maven:3.6.3'} }
    agent { docker { image 'node:13.8'} }
    stages {
        stage('build') {
            steps {
                sh 'node --version'
                //sh 'mvn --version'
                echo 'build stage'
            }
        }
    
     
        stage('compile') {
            steps {
                echo 'compile stage'
            }
        }
    
     
        stage('deploy') {
            steps {
                echo 'deploy stage'
            }
        }
    }
}
