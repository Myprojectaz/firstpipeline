pipeline {
    agent { docker { Image 'maven:3.6.3'} }

    stages {
        stage('build') {
            steps {
                sh 'mvn --version'
                echo 'build stage'
            }
        }
    
     
        stage('compile') {
            steps {
                echo 'compile stage'
            }
        }
    
     
        stage('deploy') {
            steps {
                echo 'deploy stage'
            }
        }
    }
}
